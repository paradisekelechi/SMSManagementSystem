{
  "name": "smsmanagementsystem",
  "version": "1.0.0",
  "description": "sms management system listing the population of various locations",
  "main": "app.js",
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "cross-env NODE_ENV=production babel -d ./dist ./server",
    "postbuild": "cross-env NODE_ENV=production  npm run reset:db",
    "reset:db": "cross-env NODE_ENV=production babel-node server/scripts/Reset.js",
    "reset": "cross-env NODE_ENV=development babel-node server/scripts/Reset.js",
    "start:dev": "cross-env NODE_ENV=development nodemon server/index --exec babel-node",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_ENV=test mocha --compilers babel-register --timeout 15000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paradisekelechi/SMSManagementSystem.git"
  },
  "keywords": [
    "population",
    "location",
    "management",
    "system"
  ],
  "author": "Paradise Kelechi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paradisekelechi/SMSManagementSystem/issues"
  },
  "homepage": "https://github.com/paradisekelechi/SMSManagementSystem#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "npm-run-all": "^4.1.5",
    "pg": "^7.7.1",
    "sequelize": "^4.42.0",
    "winston": "^3.1.0"
  }
}
